<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!-- font family -->
    <style name="Text.Bold" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/pretendard_bold</item>
    </style>
    <style name="Text.SemiBold" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/pretendard_semibold</item>
    </style>
    <style name="Text.Medium" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/pretendard_medium</item>
    </style>
    <style name="Text.Regular" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/pretendard_regular</item>
    </style>
    <style name="Text.Light" parent="Widget.AppCompat.TextView">
        <item name="android:fontFamily">@font/pretendard_light</item>
    </style>

    <!-- font size -->
    <style name="Text.SemiBold.10dp" parent="Text.SemiBold">
        <item name="android:textSize">10dp</item>
    </style>
    <style name="Text.Regular.10dp" parent="Text.Regular">
        <item name="android:textSize">10dp</item>
    </style>
    
    <!-- font color -->
<!--    <style name="reg.10dp."-->


</resources>